                  option(value='2048') 2048
                  option(value='2049') 2049
                  option(value='2050') 2050
                  option(value='2051') 2051
                  option(value='2052') 2052
                  option(value='2053') 2053
                  option(value='2054') 2054
                  option(value='2055') 2055
                  option(value='2056') 2056
                  option(value='2057') 2057
                  option(value='2058') 2058
                  option(value='2059') 2059
                  option(value='2060') 2060
              .month-and-year-wrapper
                h2#month-and-year.calendar-current-date
          #calendar.calendar
            .calendar-header
              #Sun Sun
              #Mon Mon
              #Tues Tue
              #Wed Wed
              #Thu Thu
              #Fri Fri
              #Sat Sat
            #calendar-body
          .quick-actions
            button#previous.movement_buttons(onclick='previous()')
              | Previous
            button#today.movement_buttons(onclick='base()')
              | Selected Date
            button#next.movement_buttons(onclick='next()')
              | Next
          br
      .daily-view-background
        .daily-view-wrapper
          #daily-view-header.daily-view-header
            h2#daily-view-date
          .daily-view-body
            .daily-view-grid-wrapper
              .daily-view-grid
