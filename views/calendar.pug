doctype html
head
  title View Calendar
  link(rel='stylesheet' type='text/css' href='../styles/calendar.css')
  link(rel='stylesheet' type='text/css' href='../styles/template.css')
.site-header
  .header-name
    h2 Full Stack
  .header-links
    a(href='../index.html')
      .link-wrapper
        | Map
    a(href='#')
      .link-wrapper
        | Calendar
.site-body
  .left-sidebar
  .site-content
    .calendar-view
      .calendar-background
        .calendar-wrapper
          form.date-form
            .heading-wrapper
              .month-wrapper
                select#month.date-dropdown(name='month' onchange='jump()')
                  option(value='' selected='' disabled='' hidden='') Month
                  option(value='0') Jan
                  option(value='1') Feb
                  option(value='2') Mar
                  option(value='3') Apr
                  option(value='4') May
                  option(value='5') Jun
                  option(value='6') Jul
                  option(value='7') Aug
                  option(value='8') Sep
                  option(value='9') Oct
                  option(value='10') Nov
                  option(value='11') Dec
              .year-wrapper
                select#year.date-dropdown(name='year' onchange='jump()')
                  option(value='Year' selected='' disabled='' hidden='') Year
                  option(value='2019') 2019
                  option(value='2020') 2020
                  option(value='2021') 2021
                  option(value='2022') 2022
                  option(value='2023') 2023
                  option(value='2024') 2024
                  option(value='2025') 2025
